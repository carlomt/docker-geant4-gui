export QT_X11_NO_MITSHM=1
export LIBGL_DEBUG=verbose

## clang
export CC=/usr/bin/clang
export CXX=/usr/bin/clang++

## DICOM Toolkit (DCMTK)
#https://dicom.offis.de/dcmtk.php.en
export DCMTK_BASE_DIR=/opt/dcmtk
export PATH=$DCMTK_BASE_DIR/bin:$PATH
export LD_LIBRARY_PATH=$DCMTK_BASE_DIR/lib64:$LD_LIBRARY_PATH

## gMocren
export GMOCREN_BASE_DIR=/opt/gmocren
alias gMocren4=$GMOCREN_BASE_DIR/gMocren4
alias gMocren4-bin=$GMOCREN_BASE_DIR/gMocren4-bin
alias gmocren=$GMOCREN_BASE_DIR/gMocren4